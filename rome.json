{
  "$schema": "https://docs.rome.tools/schemas/12.1.3/schema.json",
  "organizeImports": {
    "enabled": true
  },
  "formatter": {
    "enabled": true,
    "indentStyle": "space"
  },
  "linter": {
    "enabled": true,
    "rules": {
      "all": true,
      "style": {
        "all": true,
        "useBlockStatements": "off"
      },
      "nursery": {
        "all": true,
        "noConsoleLog": "off"
      }
    }
  }
}
