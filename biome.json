{
	"$schema": "https://biomejs.dev/schemas/1.2.2/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "space",
		"indentSize": 4
	},
	"linter": {
		"enabled": true,
		"rules": {
			"all": true,
			"style": {
				"all": true,
				"useBlockStatements": "off"
			},
			"suspicious": {
				"all": true,
				"noConsoleLog": "off"
			}
		}
	}
}